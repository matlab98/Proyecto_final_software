<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        tools:context=".MainMenu">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="148dp"
            android:layout_height="731dp"
            android:background="#1E2D3E"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:id="@+id/constraintLayout" app:layout_constraintVertical_bias="0.0">
        <ImageView
                android:layout_width="97dp"
                android:layout_height="88dp" tools:srcCompat="@tools:sample/avatars[0]"
                android:id="@+id/imageView"
                app:layout_constraintBottom_toTopOf="@+id/editText" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                android:contentDescription="TODO"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:textColor="#FFFFFF"
                android:text="servicios"
                android:ems="10"
                android:id="@+id/editText"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.25"
                android:autofillHints=""/>
        <Button
                android:text="buses"
                android:layout_width="124dp"
                android:layout_height="49dp"
                android:textAllCaps="true"
                android:textColor="#FFFFFF"
                android:background="@drawable/custom_button"
                android:id="@+id/bus" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/editText"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.117"/>
        <Button
                android:text="trenes"
                android:layout_width="124dp"
                android:layout_height="49dp"
                android:textAllCaps="true"
                android:textColor="#FFFFFF"
                android:background="@drawable/custom_button"
                android:id="@+id/tren"
                app:layout_constraintTop_toBottomOf="@+id/bus" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.4" app:layout_constraintVertical_bias="0.073"/>
        <Button
                android:text="Salir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="true"
                android:background="@drawable/custom_button"
                android:textColor="#FFFFFF"
                android:id="@+id/salirlogin"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.583" app:layout_constraintVertical_bias="0.891"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <EditText
            android:layout_width="220dp"
            android:layout_height="40dp"
            android:inputType="textPersonName"
            android:text="Transporte"
            android:ems="10"
            android:id="@+id/editText2"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.777" app:layout_constraintVertical_bias="0.007"/>
    <ImageView
            android:layout_width="181dp"
            android:layout_height="184dp" app:srcCompat="@drawable/trensabana"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.817" app:layout_constraintVertical_bias="0.202"/>
    <ImageView
            android:layout_width="181dp"
            android:layout_height="188dp" app:srcCompat="@drawable/bus1"
            android:id="@+id/imageView3"
            app:layout_constraintTop_toTopOf="@+id/editText2" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.817" app:layout_constraintVertical_bias="0.607"/>
</androidx.constraintlayout.widget.ConstraintLayout>